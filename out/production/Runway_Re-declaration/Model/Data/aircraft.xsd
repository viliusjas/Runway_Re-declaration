<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="aircrafts">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="aircraft">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element type="xs:string" name="model"/>
                            <xs:element type="xs:short" name="engineBlast"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>